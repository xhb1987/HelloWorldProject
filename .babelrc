{
    // "presets": ["react-native-stage-0"],
    // "presets": ["react-native"],
    "presets": ["babel-preset-react-native-stage-0/decorator-support"],
    "env": {
        "production": {
            "plugins": ["babel-plugin-root-import"]
        }
    },
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "src": "./src"
                }
            }
        ]
    ]
}
